<section>
  <form class="" id="new-plugin" action="/plugins" method="post">
    <div class="form-group">
      <label for="name">Name</label>
      <input type="text" name="name" value="" placeholder="Enter the plugin name.">
    </div>
    <div class="form-group">
      <label for="picUrl">Thumbnail URL</label>
      <input type="text" name="picUrl" value="" placeholder="Enter the URL.">
    </div>
    <div class="form-group">
      <label for="description">Description</label>
      <input type="text" name="description" value="" placeholder="Enter the plugin description.">
    </div>
    <div class="form-group">
      <label for="price">Price</label>
      <input type="text" name="price" value="" placeholder="Enter the plugin price.">
    </div>
    <button type="submit" name="button">Create</button>
  </form>
</section>
